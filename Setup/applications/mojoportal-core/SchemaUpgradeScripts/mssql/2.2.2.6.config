CREATE PROCEDURE [dbo].[mp_ForumPosts_UpdateSequence]

/*
Author:			Joe Audette
Created:		2007-06-07
Last Modified:	2007-06-07

*/

@PostID			int,
@ThreadSequence			int


AS

UPDATE		mp_ForumPosts

SET			ThreadSequence = @ThreadSequence


WHERE		PostID = @PostID
GO

