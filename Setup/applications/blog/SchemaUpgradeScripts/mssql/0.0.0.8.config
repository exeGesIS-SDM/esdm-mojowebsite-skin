ALTER PROCEDURE [dbo].[mp_Blog_SelectDrafts]

/*
Author:		Joe Audette
Created:	2007-12-14
Last Modified:	2009-04-03

*/
    
@ModuleID int,
@CurrentTime datetime

AS


SELECT		b.*

FROM        		mp_Blogs b

WHERE
    			(b.ModuleID = @ModuleID)  
		and (b.StartDate > @CurrentTime)

ORDER BY
   			b.StartDate DESC

GO

