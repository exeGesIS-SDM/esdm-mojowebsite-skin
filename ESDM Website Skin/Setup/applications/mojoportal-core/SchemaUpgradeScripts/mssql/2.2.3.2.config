ALTER PROCEDURE [dbo].[mp_SiteSettings_GetPageList]

/*
Author:				Joe Audette
Created:			7/27/2004
Last Modified:			3/19/2005

*/

@SiteID			int



AS
SELECT  
    				*
    
FROM    
    				mp_Pages
    
WHERE   
    				SiteID = @SiteID
					

ORDER BY			ParentID, PageOrder, PageName

GO
