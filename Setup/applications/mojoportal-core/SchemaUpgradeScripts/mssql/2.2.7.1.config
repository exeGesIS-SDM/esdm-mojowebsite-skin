CREATE PROCEDURE [dbo].[mp_SharedFilesHistory_DeleteByItemID]

/*
Author:   			Joe Audette
Created: 			2008-08-29
Last Modified: 		2008-08-29

*/

@ItemID int

AS

DELETE FROM [dbo].[mp_SharedFilesHistory]
WHERE
	[ItemID] = @ItemID

GO


