CREATE PROCEDURE [dbo].[mp_CommerceReport_MoveOrder]

/*
Author:   			Joe Audette
Created: 			2012-01-21
Last Modified: 		2012-02-21
*/

@OrderGuid uniqueidentifier,
@UserGuid uniqueidentifier

AS

UPDATE [dbo].[mp_CommerceReport]
SET UserGuid = @UserGuid
WHERE
	[OrderGuid] = @OrderGuid


GO

