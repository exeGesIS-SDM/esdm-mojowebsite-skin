ON			
		c.[CartGuid] = t2.[CartGuid]
		
LEFT OUTER JOIN 
		[dbo].mp_Users u
ON 
		u.UserGuid = c.UserGuid
		
WHERE
		t2.IndexID > @PageLowerBound 
		AND t2.IndexID < @PageUpperBound
		
ORDER BY t2.IndexID

