ALTER PROCEDURE [dbo].[mp_HtmlContent_Select]

/*
Author:			Joe Audettte
Created:		2004-12-23
Last Modified:	2008-08-28

*/


@ModuleID int,
@BeginDate datetime

AS
SELECT  	*

FROM		mp_HtmlContent

WHERE	ModuleID = @ModuleID
		AND BeginDate <= @BeginDate
		AND EndDate >= @BeginDate

ORDER BY BeginDate DESC

GO


