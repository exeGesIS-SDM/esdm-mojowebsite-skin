ALTER PROCEDURE [dbo].[mp_SurveyResponses_GetFirst]

/*
Author:   			Rob Henry
Created: 			2007-10-21
Last Modified: 		2007-10-21
*/

@SurveyGuid uniqueidentifier

AS

SELECT TOP 1
[ResponseGuid],
[SurveyGuid],
[UserGuid],
[SubmissionDate],
[Annonymous],
[Complete]

FROM
[dbo].[mp_SurveyResponses]

WHERE
Complete = 1
AND
SurveyGuid = @SurveyGuid
ORDER BY
SubmissionDate,
ResponseGuid


GO

ALTER PROCEDURE [dbo].[mp_SurveyResponses_GetNext]

/*
Author:   			Rob Henry
Created: 			2007-10-21
Last Modified: 		2007-10-21
*/

@ResponseGuid uniqueidentifier

AS

SELECT TOP 1
		[ResponseGuid],
		[SurveyGuid],
		[UserGuid],
		[SubmissionDate],
		[Annonymous],
		[Complete]
		
FROM
		[dbo].[mp_SurveyResponses]
		
WHERE
		SubmissionDate > (
		SELECT 
				SubmissionDate 
		FROM
				mp_SurveyResponses
		WHERE 
				ResponseGuid = @ResponseGuid
		)
AND
		SurveyGuid = (
		SELECT 
				SurveyGuid 
		FROM
				mp_SurveyResponses
		WHERE
				ResponseGuid = @ResponseGuid
		)
AND
		Complete = 1
ORDER BY
		SubmissionDate,
		ResponseGuid
		
GO


