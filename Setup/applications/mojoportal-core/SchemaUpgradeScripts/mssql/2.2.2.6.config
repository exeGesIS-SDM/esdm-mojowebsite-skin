CREATE PROCEDURE [dbo].[mp_FriendlyUrls_SelectBySiteID]

/*
Author:   			Joe Audette
Created: 			2007/06/07
Last Modified: 		2007/06/07
*/

@SiteID		int

AS




SELECT
		*
		
FROM
		[dbo].[mp_FriendlyUrls]

WHERE	SiteID = @SiteID
ORDER BY FriendlyUrl
GO
