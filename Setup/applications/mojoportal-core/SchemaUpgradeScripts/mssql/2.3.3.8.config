ALTER PROCEDURE [dbo].[mp_Pages_SelectChildPages]

/*
Author:				Joe Audette
Created:			2005-02-20
Last Modified:		2010-02-10

*/

@SiteID int,
@ParentID		int


AS


SELECT	*

FROM		mp_Pages

WHERE	ParentID = @ParentID
		AND SiteID = @SiteID

ORDER BY PageOrder

GO

