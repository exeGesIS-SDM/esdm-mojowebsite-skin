ALTER PROCEDURE [dbo].[mp_FileAttachment_Insert]

/*
Author:   			Joe Audette
Created: 			2009-03-08
Last Modified: 		2012-09-19
*/

@RowGuid uniqueidentifier,
@SiteGuid uniqueidentifier,
@ModuleGuid uniqueidentifier,
@ItemGuid uniqueidentifier,
@SpecialGuid1 uniqueidentifier,
@SpecialGuid2 uniqueidentifier,
@ServerFileName nvarchar(255),
@FileName nvarchar(255),
@ContentTitle nvarchar(255),
@ContentLength bigint,
@ContentType nvarchar(50),
@CreatedUtc datetime,
@CreatedBy uniqueidentifier

	
AS

INSERT INTO 	[dbo].[mp_FileAttachment] 
(
				[RowGuid],
				[SiteGuid],
				[ModuleGuid],
				[ItemGuid],
				[SpecialGuid1],
				[SpecialGuid2],
				[ServerFileName],
				[FileName],
				ContentTitle,
				ContentLength,
				ContentType,
				[CreatedUtc],
				[CreatedBy]
) 

VALUES 
(
				@RowGuid,
				@SiteGuid,
				@ModuleGuid,
				@ItemGuid,
				@SpecialGuid1,
				@SpecialGuid2,
				@ServerFileName,
				@FileName,
				@ContentTitle,
				@ContentLength,
				@ContentType,
				@CreatedUtc,
				@CreatedBy
				
)


GO

ALTER PROCEDURE [dbo].[mp_FileAttachment_Update]

/*
Author:   			Joe Audette
Created: 			2009-03-08
Last Modified: 		2012-09-19
*/
	
@RowGuid uniqueidentifier, 
@ServerFileName nvarchar(255), 
@FileName nvarchar(255),
@ContentTitle nvarchar(255),
@ContentLength bigint,
@ContentType nvarchar(50)


AS

UPDATE 		[dbo].[mp_FileAttachment] 

SET
			
			[ServerFileName] = @ServerFileName,
			[FileName] = @FileName,
			ContentTitle = @ContentTitle,
			ContentLength = @ContentLength,
			ContentType = @ContentType
			
WHERE
			[RowGuid] = @RowGuid


GO

